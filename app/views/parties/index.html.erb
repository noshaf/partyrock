<div class="span12">
	<div class="well" id="spotify">
		<h1>Search for a Party</h1><br>
		<%= form_for (@party), :remote => true do |f| %>
			<%= f.search_field :name %><br>
		<% end %>
		<div class="gary">
		</div>
	</div>
</div>

<script>


	$("#party_name").keyup(

		function(){
			var value = $(this).val();
			$.ajax({
			    type:"GET",
			    dataType: 'script',
			    url: '<%= "#{@url}?search=" %>' + value,
			    complete: function () {
			      //console.log('complete');
			    },
			    error: function (jqXHR, textStatus, errorThrown) {
			      //console.log(jqXHR.responseText);
			      //console.log(textStatus);
			      //console.log(errorThrown);
			    },
			    success: function(data) {
			      //console.log("Success");
			    }
			})
		}

	);


</script>

