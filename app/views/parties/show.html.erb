<div class="span12">
	<div class="well" id="spotify">
		<h1><%= @party.name %></h1>
		<iframe src="https://embed.spotify.com/?uri=spotify:trackset:<%="#{@party.name}:#{@party_tracks}"%>" frameborder="0" allowtransparency="true"></iframe>
	</div>
</div>

<div class="span6">
	<div class="well" id="spotify-playlist">
		<h3> Playist </h3>
		
		<div class="playlist">
			<table class='table table-striped'>
	        	<tbody>
		          <% 20.times do |party| %>
		          	<tr>
		            	<td>
		              		1.
			            </td>
			            <td>
			               	Name
			            </td>
			            <td>
			               	Artists
			            </td>
			            <td>
			               	
			            </td>
		          	</tr>
		          <% end %>
	        	</tbody>
	      	</table>
		</div>
	</div>
</div>

<div class="span6">
	<div class="well" id="spotify">

		<h3>Add a Song</h3>
		<%= form_for [@song], :remote => true do |f| %>
		  <%= f.label :name %>
		  <%= f.text_field :name %><br>
		  <%= f.submit %>
		<% end %>

		<div class="search">
			Songs...
		</div>
	
	</div>
</div>
